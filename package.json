{
  "name": "daniel.heene.io",
  "version": "0.1.0",
  "private": true,
  "description": "Personal Website for daniel.heene.io",
  "repository": "https://github.com/danielheene/danielheene.git",
  "license": "MIT",
  "author": "Daniel Heene <daniel@heene.io>",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "postinstall": "yarn run-s pkg:*",
    "pkg:dedupe": "yarn yarn-deduplicate -s fewer yarn.lock",
    "pkg:sort": "yarn sort-package-json",
    "pkg:types": "yarn typesync | grep -q 'npm install' && yarn install --ignore-scripts || exit 0",
    "prettier": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx,vue}\""
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@emotion/css": "11.7.1",
    "@emotion/react": "11.7.1",
    "@emotion/serialize": "1.0.2",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.6.0",
    "@headlessui/react": "1.4.1",
    "canvas-confetti": "1.5.1",
    "date-fns": "2.24.0",
    "github-colors": "2.2.20",
    "next": "12.0.10",
    "next-mdx-remote": "3.0.6",
    "next-seo": "5.1.0",
    "next-themes": "0.0.15",
    "nprogress": "0.2.0",
    "ogl-typescript": "0.1.40",
    "preact": "10.6.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-use": "17.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.4",
    "@emotion/babel-plugin": "11.3.0",
    "@iconify/react": "3.1.3",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "0.5.2",
    "@types/babel-plugin-macros": "2.8.5",
    "@types/babel__core": "7.1.18",
    "@types/canvas-confetti": "1.4.2",
    "@types/node": "17.0.18",
    "@types/nprogress": "0.2.0",
    "@types/prettier": "2.4.4",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.9",
    "autoprefixer": "10.4.2",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-twin": "1.0.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.6",
    "prettier": "2.4.1",
    "sort-package-json": "1.54.0",
    "tailwindcss": "3.0.23",
    "twin.macro": "2.7.0",
    "typescript": "4.4.3",
    "typesync": "0.8.0",
    "yarn-deduplicate": "3.1.0"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  }
}
